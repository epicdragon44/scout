#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/danielwei15/Documents/Development/scout/scout/node_modules/.pnpm/plasmo@0.89.4_react-dom@18.2.0_react@18.2.0/node_modules/plasmo/bin/node_modules:/Users/danielwei15/Documents/Development/scout/scout/node_modules/.pnpm/plasmo@0.89.4_react-dom@18.2.0_react@18.2.0/node_modules/plasmo/node_modules:/Users/danielwei15/Documents/Development/scout/scout/node_modules/.pnpm/plasmo@0.89.4_react-dom@18.2.0_react@18.2.0/node_modules:/Users/danielwei15/Documents/Development/scout/scout/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/danielwei15/Documents/Development/scout/scout/node_modules/.pnpm/plasmo@0.89.4_react-dom@18.2.0_react@18.2.0/node_modules/plasmo/bin/node_modules:/Users/danielwei15/Documents/Development/scout/scout/node_modules/.pnpm/plasmo@0.89.4_react-dom@18.2.0_react@18.2.0/node_modules/plasmo/node_modules:/Users/danielwei15/Documents/Development/scout/scout/node_modules/.pnpm/plasmo@0.89.4_react-dom@18.2.0_react@18.2.0/node_modules:/Users/danielwei15/Documents/Development/scout/scout/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../plasmo/bin/index.mjs" "$@"
else
  exec node  "$basedir/../plasmo/bin/index.mjs" "$@"
fi
